{
    "name": "tab-tracker",
    "description": "",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "scripting",
        "tabs",
        "windows",
        "storage",
        "unlimitedStorage"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "./src/background.js",
        "type": "module"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "testhis.json"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}